@prefix : <http://project-first.eu/ontology#>.
@prefix OWL: <http://www.w3.org/2002/07/owl#>.
@prefix RDF: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix RDFS: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix XSD: <http://www.w3.org/2001/XMLSchema#>.

#classes
:Digital_Currency
    a OWL:Class;
    RDFS:label "Digital_Currency";
    RDFS:subClassOf :Financial_Instrument.
   
:Digital_Currency_Market
    a OWL:Class;
    RDFS:label "Digital_Currency_Market";
    RDFS:subClassOf :Digital_Currency.

:Cryptocoin
    a OWL:Class;
    RDFS:label "Digital_Currency";
    RDFS:subClassOf :Digital_Currency.
   
# instances

:glo_digital_currency_term
	:identifiedBy :glo_digital_currency_term_Gazetteer;
	a :Digital_Currency;
	RDFS:label "Digital currency term".

:glo_digital_currency_term_Gazetteer
	:term "Digital currency",
		"virtual currency",
        "cryptocurrency",
        "P2P digital currency",
        "Cryptocoin";
	a :Gazetteer.

